<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.miaxis.mr800test">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BaseInfoActivity" />
        <activity android:name=".activity.PasswordActivity" />
        <activity android:name=".activity.IOActivity" />
        <activity android:name=".activity.GridTestActivity" />
        <activity android:name=".activity.ConfigActivity" />
        <activity android:name=".activity.ScreenTestActivity" />
        <activity android:name=".activity.KeyBoardActivity" />
        <activity android:name=".activity.CameraActivity" />
        <activity android:name=".activity.VoiceActivity" />
        <activity android:name=".activity.USBActivity" />
        <activity android:name=".activity.PortActivity" />
        <activity android:name=".activity.PenActivity" />
        <activity android:name=".activity.SDActivity" />
        <activity android:name=".activity.CountActivity" />
        <activity android:name=".activity.ICActivity" />
        <activity android:name=".activity.FICActivity" />
        <activity android:name=".activity.MicActivity" />
        <activity android:name=".activity.WifiActivity" />
        <activity android:name=".activity.IDActivity"></activity>
    </application>

</manifest>